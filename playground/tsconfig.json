{
  "compilerOptions": {
    "baseUrl": "./",
    "noEmit": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "paths": {
      "react-native-eva-icons": [
        "../lib"
      ],
      "react-native-eva-icons/*": [
        "../lib/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
