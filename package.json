{
  "name": "react-native-eva-icons",
  "version": "1.0.0-beta.1",
  "description": "Eva Icons for React Native",
  "scripts": {
    "start": "npm run gen:prepare && npm run gen:all && npm run lint:all",
    "build": "tsc",
    "clean:build": "rimraf ./dist",
    "clean:lib": "rimraf ./lib/icons",
    "lint:tslint": "tslint -c ./tslint.json ./lib/icons/**/*.{ts,tsx} --fix",
    "lint:prettier": "prettier ./lib/icons/**/*.{ts,tsx} --write --single-quote --jsx-single-quote",
    "lint:all": "npm run lint:prettier && npm run lint:tslint",
    "gen:prepare": "mkdir ./lib/icons",
    "gen:fill": "ts-node ./scripts/generate.ts ./node_modules/eva-icons/fill/svg ./lib/icons",
    "gen:outline": "ts-node ./scripts/generate.ts ./node_modules/eva-icons/outline/svg ./lib/icons",
    "gen:index": "ts-node ./scripts/generateIndex.ts ./lib/icons",
    "gen:all": "npm run gen:fill && npm run gen:outline && npm run gen:index",
    "release:prepare": "npm run clean:build && npm run build",
    "release": "npm run clean:build && npm run build",
    "publish": "npm run release:prepare && npm publish --access=public ./dist/tsc-out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artyorsh/react-native-eva-icons.git"
  },
  "keywords": [
    "react-native",
    "eva-icons",
    "svg"
  ],
  "author": "artyorsh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artyorsh/react-native-eva-icons/issues"
  },
  "homepage": "https://github.com/artyorsh/react-native-eva-icons#readme",
  "devDependencies": {
    "@types/node": "^12.0.4",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.60",
    "@types/stringify-object": "^3.2.0",
    "eva-icons": "^1.1.1",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-native": "^0.59.9",
    "react-native-svg": "^9.4.0",
    "rimraf": "^2.6.3",
    "stringify-object": "^3.3.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1",
    "change-case": "^3.1.0"
  }
}
